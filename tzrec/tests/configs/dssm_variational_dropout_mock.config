train_input_path: ""
eval_input_path: ""
model_dir: "experiments/dssm_mock"
train_config {
    sparse_optimizer {
        adagrad_optimizer {
            lr: 0.001
        }
        constant_learning_rate {
        }
    }
    dense_optimizer {
        adam_optimizer {
            lr: 0.001
        }
        constant_learning_rate {
        }
    }
    num_epochs: 8
}
eval_config {
}
data_config {
    batch_size: 8192
    dataset_type: ParquetDataset
    label_fields: "clk"
    num_workers: 8
    negative_sampler {
        input_path: "odps://{PROJECT}/tables/taobao_ad_feature_gl_bucketized_v1"
        num_sample: 1024
        attr_fields: "item_id"
        attr_fields: "item_id_1"
        attr_fields: "item_id_2"
        attr_fields: "item_raw_1"
        attr_fields: "item_raw_2"
        attr_fields: "item_raw_3"
        item_id_field: "item_id"
        attr_delimiter: "\x02"
    }
}
feature_configs {
    id_feature {
        feature_name: "user_id"
        num_buckets: 1000000
        embedding_dim: 16
    }
}
feature_configs {
    id_feature {
        feature_name: "user_id_1"
        num_buckets: 10000
        embedding_dim: 16
    }
}
feature_configs {
    id_feature {
        feature_name: "user_id_2"
        num_buckets: 1000
        embedding_dim: 8
    }
}
feature_configs {
    id_feature {
        feature_name: "user_id_3"
        num_buckets: 100
        embedding_dim: 16
        embedding_name: "user_id_3_emb"
    }
}
feature_configs {
    id_feature {
        feature_name: "user_id_4"
        num_buckets: 100
        embedding_dim: 16
        embedding_name: "user_id_3_emb"
    }
}
feature_configs {
    raw_feature {
        feature_name: "user_raw_1"
        boundaries: [0.1, 0.2, 0.3, 0.4]
        embedding_dim: 16
    }
}
feature_configs {
    raw_feature {
        feature_name: "user_raw_2"
    }
}
feature_configs {
    raw_feature {
        feature_name: "user_raw_3"
        value_dim: 4
    }
}
feature_configs {
    id_feature {
        feature_name: "item_id"
        num_buckets: 1000000
        embedding_dim: 16
    }
}
feature_configs {
    id_feature {
        feature_name: "item_id_1"
        num_buckets: 10000
        embedding_dim: 16
    }
}
feature_configs {
    id_feature {
        feature_name: "item_id_2"
        num_buckets: 1000
        embedding_dim: 8
    }
}
feature_configs {
    raw_feature {
        feature_name: "item_raw_1"
        boundaries: [0.1, 0.2, 0.3, 0.4]
        embedding_dim: 16
    }
}
feature_configs {
    raw_feature {
        feature_name: "item_raw_2"
    }
}
feature_configs {
    raw_feature {
        feature_name: "item_raw_3"
        value_dim: 4
    }
}
model_config {
    feature_groups {
        group_name: "user"
        feature_names: "user_id"
        feature_names: "user_id_1"
        feature_names: "user_id_2"
        feature_names: "user_id_3"
        feature_names: "user_id_4"
        feature_names: "user_raw_1"
        feature_names: "user_raw_2"
        feature_names: "user_raw_3"
        group_type: DEEP
    }
    feature_groups {
        group_name: "item"
        feature_names: "item_id"
        feature_names: "item_id_1"
        feature_names: "item_id_2"
        feature_names: "item_raw_1"
        feature_names: "item_raw_2"
        feature_names: "item_raw_3"
        group_type: DEEP
    }
    variational_dropout {
    }
    dssm {
        user_tower {
            input: 'user'
            mlp {
                hidden_units: [512, 256, 128]
            }
        }
        item_tower {
            input: 'item'
            mlp {
                hidden_units: [512, 256, 128]
            }
        }
        output_dim: 64
    }
    metrics {
        recall_at_k {
            top_k: 1
        }
    }
    metrics {
        recall_at_k {
            top_k: 5
        }
    }
    losses {
        softmax_cross_entropy {}
    }
}
